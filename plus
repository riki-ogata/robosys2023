import sys

ans1 = 0
ans2 =0

for line in sys.stdin:
    try:
       ans1 += int(line)
       ans2 -= int(line)
    except ValueError:
       print("Error: Invalid input.")
       sys.exit(1)

print(ans1)
print(ans2)
